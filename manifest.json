{
  "name": "Small Business Alternative",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

  "icons": {
    "16":"icon16.jpg", "32": "icon32.jpg"
  },

  "browser_action": {
    "default_icon": { "16":"icon16.jpg", "32": "icon32.jpg"
    },
    "default_title": "Small Biz",
    "default_popup": "popup.html"
  }
}
